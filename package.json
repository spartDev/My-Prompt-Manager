{
  "name": "prompt-library-extension",
  "version": "1.4.2",
  "description": "Chrome extension for managing personal prompt library",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "package": "node package-extension.cjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "if [ \"$CI\" = \"true\" ] || [ -n \"$GITHUB_ACTIONS\" ] || [ -n \"$JENKINS_URL\" ] || [ -n \"$BUILDKITE\" ]; then echo 'CI detected, skipping husky'; else husky; fi",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.0",
    "@playwright/test": "1.55.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/chrome": "0.1.12",
    "@types/jsdom": "21.1.7",
    "@types/node": "24.5.2",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.13",
    "autoprefixer": "10.4.21",
    "eslint": "9.35.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "9.1.7",
    "jsdom": "27.0.0",
    "lint-staged": "15.5.2",
    "playwright": "1.55.1",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vite": "6.3.6",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@types/dompurify": "3.2.0",
    "dompurify": "3.2.7",
    "lz-string": "1.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "uuid": "13.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  }
}
