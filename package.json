{
  "name": "prompt-library-extension",
  "version": "1.8.2",
  "description": "Chrome extension for managing personal prompt library",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "package": "node package-extension.cjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "if [ \"$CI\" = \"true\" ] || [ -n \"$GITHUB_ACTIONS\" ] || [ -n \"$JENKINS_URL\" ] || [ -n \"$BUILDKITE\" ]; then echo 'CI detected, skipping husky'; else husky; fi",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.3.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/chrome": "0.1.32",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.6",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.5",
    "@vitest/eslint-plugin": "1.3.26",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-playwright": "2.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "happy-dom": "20.0.11",
    "husky": "9.1.7",
    "jsdom": "27.0.1",
    "lint-staged": "16.2.7",
    "playwright": "1.57.0",
    "postcss": "8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vite": "7.3.1",
    "vitest": "4.0.5"
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.4",
    "dompurify": "3.3.1",
    "lz-string": "1.5.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "uuid": "13.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  }
}
